<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.basicframe.demo.mapper.UserMapper">

	<!-- 通用查询映射结果 -->
	<resultMap id="BaseResultMap" type="com.basicframe.system.entity.User">
		<id column="ID" property="id" />
		<result column="USER_NAME" property="userName" />
		<result column="PASSWORD" property="password" />
		<result column="NAME" property="name" />
		<result column="SEX" property="sex" />
		<result column="ADDRESS" property="address" />
		<result column="DIRTHDAY" property="dirthday" />
		<result column="MOBILE" property="mobile" />
		<result column="MAIL" property="mail" />
		<result column="WECHAT" property="wechat" />
		<result column="SKETCH" property="sketch" />
		<result column="ENCRYPT_SALT" property="encryptSalt" />
		<result column="STATUS" property="status" />
		<result column="CREATE_TIME" property="createTime" />
		<result column="OPERATE_TIME" property="operateTime" />
	</resultMap>

</mapper>
